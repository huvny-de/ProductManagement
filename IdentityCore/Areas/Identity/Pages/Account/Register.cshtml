@page
@model RegisterModel
@{
    ViewData["Title"] = "Register";
    Layout = "~/Areas/Identity/Pages/_LayoutPhanHeQTUser.cshtml";

}

<h1>@ViewData["Title"]</h1>

<form asp-route-returnUrl="@Model.ReturnUrl" method="post">
    FirstName:<input asp-for="Input.FirstName" />
    LastName:<input asp-for="Input.LastName" />
    Phone:<input asp-for="Input.Phone" />
    Email:<input asp-for="Input.Email" />
    Password:<input asp-for="Input.Password" />
    Confirm Password:<input asp-for="Input.ConfirmPassword" />

    <button type="submit">Register</button>

</form>